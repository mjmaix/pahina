{
  "expo": {
    "name": "Pahina",
    "description": "Shop and sell notes. Digital shopping and selling platforms for professional and student notes.",
    "slug": "pahina",
    "privacy": "unlisted",
    "sdkVersion": "33.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#E2A4C9",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E2A4C9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mjabadilla.pahina"
    },
    "android": {
      "package": "com.mjabadilla.pahina"
    },
    "packagerOpts": {
      "config": "metro.config.js"
    }
  }
}

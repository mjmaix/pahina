    "@types/yup": "0.26.22",
    "babel-plugin-styled-components": "1.10.6",
    "babel-preset-expo": "5.2.0",
    "eslint": "^6.0.1",
    "patch-package": "6.1.2",
    "react-native-testing-library": "1.11.1",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",

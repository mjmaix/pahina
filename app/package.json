{
  "name": "@pahina/app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "start": "expo start --clear",
    "expo:android": "expo start --android --clear",
    "expo:ios": "expo start --ios --clear",
    "expo:web": "expo start --web --clear",
    "expo:eject": "expo eject",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "clean:folders": "rm -rf node_modules /tmp/metro-bundler-cache-* /tmp/haste-map-react-native-packer-*",
